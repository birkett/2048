{
    "env": {
        "browser": true,
        "node": false,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "max-len": ["error", 100]
    }
}
